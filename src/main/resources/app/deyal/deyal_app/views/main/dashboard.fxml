<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane styleClass="root" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.deyal.deyal_app.controllers.DashboardController">
    <TableView fx:id="dashboardTableView" prefHeight="600.0" tableMenuButtonVisible="true">
        <columns>
            <TableColumn fx:id="missionTitleTableColumn" editable="false" prefWidth="150.0"
                         text="Mission Title"/>
            <TableColumn fx:id="missionLevelTableColumn" editable="false" prefWidth="100.0"
                         text="Level"/>
            <TableColumn fx:id="missionCreatorTableColumn" editable="false" prefWidth="150.0"
                         text="Creator"/>
            <TableColumn fx:id="missionDescriptionTableColumn" editable="false" prefWidth="400.0"
                         text="Short Description"/>
        </columns>
    </TableView>
</AnchorPane>
